//feladat: prim szamok 100ig kb

oszto = 0;
function prim(a){
    for(let i = 1; i <= a; i++){
        if(a % i == 0){
            oszto+=1
        }
    }
    return(oszto==2 ? "prim" : "nem prim")
}
document.write(prim(10))

//fugven ykifejezess addj meg eegy születesi evet es terj vissza az életkorral
const eletkor = function(a){
    return 2022-a
}
console.log(eletkor(2010))

